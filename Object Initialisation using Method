package com.company;

import com.sun.jdi.Value;
import org.w3c.dom.ls.LSOutput;

import java.security.spec.RSAOtherPrimeInfo;
import java.util.*;
class Student
{
    int roll_no;
    String student_name;
    public void details(int roll,String name)
    {
        roll_no=roll;
        student_name=name;
        System.out.println(roll_no+" "+student_name);
    }
}

public class Main
{
    public static void main(String[] args)
    {
        Student obj=new Student();
        obj.details(1,"Ram"); //Object initialisation using method



    }


}







